{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "generate": {
      "dependsOn": ["^generate"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "tscheck": {
      "dependsOn": ["^tscheck"],
      "outputs": [],
      "inputs": ["$TURBO_DEFAULT$"]
    }
  },
  "envMode": "loose"
}
