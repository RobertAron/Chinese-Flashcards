{
  "typescript.tsdk": "node_modules/typescript/lib",
  "tailwindCSS.classAttributes": [".*ClassName", ".*Classes"],
  "tailwindCSS.experimental.classRegex": [
    ["[cC]lasses\\s*(?::[^=]+)?=([^;]*);", "'([^']*)'"],
    ["[cC]lasses\\s*(?::[^=]+)?=([^;]*);", "\"([^\"]*)\""],
    ["[cC]lasses\\s*(?::[^=]+)?=([^;]*);", "\\`([^\\`]*)\\`"]
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/.vscode/**": true
  },
  "prisma.pinToPrisma6": true
}
